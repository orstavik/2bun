{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "**/*",
        "function": "twoBun"
      },
      {
        "source": "**/index.html",
        "function": "twoBun"
      },
      {
        "source": "**/favicon.ico",
        "destination": "/favicon.ico"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}